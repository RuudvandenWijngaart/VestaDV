	template make_result
	{
		// begin case parameter
		parameter<string> type;
		unit<uint32> optionSet;
		attribute<string> GDAL_LayerCreationOptions (optionSet);
		// end case parameter

		parameter<string> GDAL_Driver := type == 'GPKG' ? 'GPKG' : 'CSV';
		parameter<string> filename    := = 'result_info/filename_' + type;

		parameter<string> make_geopackage := 'OK', ExplicitSuppliers = 
			"result/ligplaats;result/ligplaats_status_code;result/ligplaats_nevenadres;"
			"result/standplaats;result/standplaats_status_code;result/standplaats_nevenadres;"
			"result/pand;result/pand/gebruiksdoelen;result/pand_status_code;result/pand_woningtype;"
			"result/vbo;result/vbo/gebruiksdoelen;result/vbo_status_code;result/vbo_gerelateerdPand;result/vbo_nevenadres;"
			"result/nummeraanduiding;result/nummeraanduiding_status_code;"
			"result/openbareruimte;result/openbareruimte_status_code;"
			"result/woonplaats;result/woonplaats_status_code;"
			"result_gpkg/result/adres"
			;

		container result
		: StorageName    = "= filename"
		, StorageType     = "gdalwrite.vect"
		, StorageReadOnly = "False"
		{
			unit<uint32> ligplaats := .../ligplaats/result
			{
				attribute<string>         identificatie       := ..../ligplaats/result/identificatie;
				attribute<geometries/rdc> geometry     (poly) := ..../ligplaats/result/geometry_mm[geometries/rdc];
				attribute<string>         nummeraanduiding_id := ..../ligplaats/result/nummeraanduiding_id;

				container meta := meta_write_template(ligplaats, ..../ligplaats/result/meta )
				{
					attribute<ligplaats_status_code> StatusCode (ligplaats):= rlookup(lowercase(Status), lowercase(ligplaats_status_code/label));
				}
			}
			
			unit<uint8> ligplaats_status_code := typeringen/status/ligplaats 
			{
				attribute<uint8>  code  := id(.);
				attribute<string> label := typeringen/status/ligplaats/label;
			}

			unit<uint32> ligplaats_nevenadres := .../ligplaats/result/nevenadres
			{
				attribute<string> nummeraanduiding_id := ..../ligplaats/result/nevenadres/nummeraanduiding_id;
				attribute<string> identificatie       := ..../ligplaats/result/nevenadres/identificatie;
			}


			unit<uint32> standplaats := .../standplaats/result
			{
				attribute<string>         identificatie       := ..../standplaats/result/identificatie;
				attribute<geometries/rdc> geometry     (poly) := ..../standplaats/result/geometry_mm[geometries/rdc];
				attribute<string>         nummeraanduiding_id := ..../standplaats/result/nummeraanduiding_id;

				container meta := meta_write_template(standplaats, ..../standplaats/result/meta )
				{
					attribute<standplaats_status_code> StatusCode (standplaats):= rlookup(lowercase(Status), lowercase(standplaats_status_code/label));
				}
			}

			unit<uint8> standplaats_status_code := typeringen/status/standplaats 
			{
				attribute<uint8>  code  := id(.);
				attribute<string> label := typeringen/status/standplaats/label;
			}

			unit<uint32> standplaats_nevenadres := .../standplaats/result/nevenadres
			{
				attribute<string> nummeraanduiding_id := ..../standplaats/result/nevenadres/nummeraanduiding_id;
				attribute<string> identificatie       := ..../standplaats/result/nevenadres/identificatie;
			}

			unit<uint32> pand := .../pand/result
			{
				attribute<string>                                              identificatie   := ..../pand/result/identificatie;
				attribute<geometries/rdc>                                      geometry (poly) := ..../pand/result/geometry_mm[geometries/rdc];
				attribute<eenheden/jaar>                                       bouwjaar        := ..../pand/result/bouwjaar;
				attribute<afleidingen/pand_attributen/geometrie/type_woonpand> woningtype      := ..../pand/result/woonpand_type;

				container meta := meta_write_template(pand, ..../pand/result/meta )
				{
					attribute<pand_status_code> StatusCode (pand):= rlookup(lowercase(Status), lowercase(pand_status_code/label));
				}

				container gebruiksdoelen := for_each_nedv(
						'nr_'+ typeringen/gebruiksdoel_vbo/name
						, 'sum(...../vbo/result/gebruiksdoelen/' + typeringen/gebruiksdoel_vbo/name + ' ? 1 : 0 , ...../vbo/result/pand_rel)'
						, pand
						, uint32
					);
			}

			unit<uint8> pand_status_code := typeringen/status/pand 
			{
				attribute<uint8>  code  := id(.);
				attribute<string> label := typeringen/status/pand/label;
			}

			unit<uint8> pand_woningtype := afleidingen/pand_attributen/geometrie/type_woonpand
			{
				attribute<uint8>  code  := id(.);
				attribute<string> label := afleidingen/pand_attributen/geometrie/type_woonpand/Label;
			}

			unit<uint32> vbo := .../vbo/result
			{
				attribute<string>         identificatie       := ..../vbo/result/identificatie;
				attribute<geometries/rdc> geometry            := ..../vbo/result/geometry_mm[geometries/rdc];
				attribute<int32>          oppervlakte         := ..../vbo/result/oppervlakte;
				attribute<string>         nummeraanduiding_id := ..../vbo/result/nummeraanduiding_id;
				attribute<string>         pand_id             := ..../vbo/result/pand_id;
				attribute<string>         pand_id_geom        := ..../vbo/result/pand_id_geom;

				container meta := meta_write_template(vbo, ..../vbo/result/meta )
				{
					attribute<vbo_status_code> StatusCode (vbo):= rlookup(lowercase(Status), lowercase(vbo_status_code/label));
				}

				container gebruiksdoelen := for_each_nedv(
						typeringen/gebruiksdoel_vbo/name
						, 'last(...../vbo/impl/selectie/gebruiksdoelen/' + typeringen/gebruiksdoel_vbo/name + ', ...../vbo/impl/selectie/result_rel)'
						, vbo
						, bool
					);
			}

			unit<uint8> vbo_status_code := typeringen/status/vbo 
			{
				attribute<uint8>  code  := id(.);
				attribute<string> label := typeringen/status/vbo/label;
			}

			unit<uint32> vbo_gerelateerdPand := .../vbo/result/gerelateerdPand
			{
				attribute<string> identificatie := ..../vbo/result/gerelateerdPand/identificatie;
				attribute<string> pand_id       := ..../vbo/result/gerelateerdPand/pand_id;
			}

			unit<uint32> vbo_nevenadres := .../vbo/result/nevenadres
			{
				attribute<string> identificatie       := ..../vbo/result/nevenadres/identificatie;
				attribute<string> nummeraanduiding_id := ..../vbo/result/nevenadres/nummeraanduiding_id;
			}

			unit<uint32> nummeraanduiding := .../nummeraanduiding/result
			{
				attribute<string> identificatie           := ..../nummeraanduiding/result/identificatie;
				attribute<int32>  huisnummer              := ..../nummeraanduiding/result/huisnummer;
				attribute<string> huisletter              := ..../nummeraanduiding/result/huisletter;
				attribute<string> toevoeging              := ..../nummeraanduiding/result/toevoeging;
