container voorbeeldwoningen
{
	unit<uint32> vbw_data: StorageName = "%projDir%/data/voorbeeldwoningen_NTA8800.csv", StorageType = "gdal2.vect", StorageReadOnly = "True";
	
	unit<uint32> vbw := select_with_attr_by_cond(vbw_data, IsPositive(uint32(vbw_data/Nr)))
	{
		container Variabelen
		{
			attribute<uint32>	Nr (vbw) := vbw/Nr[uint32];
			attribute<Classifications/EnergieLabel> EnergieLabel (vbw) := rlookup(vbw/EnergieLabel,Classifications/EnergieLabel/label);
			attribute<uint32> BouwJaar (vbw) := vbw/bouwjaar[uint32];
		}
	}
}