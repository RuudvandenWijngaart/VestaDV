	container LokalePraktijkFactor
	{
		container class
		{
			unit<uint32> BW := combine_unit(Geography/RegioIndelingen/buurt, Classifications/woningtypeBestaand);
			unit<uint32> GW := combine_unit(Geography/RegioIndelingen/gemeente, Geography/RegioIndelingen/wijk);
			unit<uint32> GWB := combine_unit(GW, Geography/RegioIndelingen/buurt);
			unit<uint32> GWBW := combine_unit(GWB, Classifications/woningtypeBestaand);
		}
		
		unit<uint32> src
		:	StorageName     = "%EnergieDataProjDir%/20241016_Lokale_Praktijkfactor_Referentieverbruik_2.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True";
		
		unit<uint32> data := src
		{
			attribute<string> 								GemeenteCode				:= src/gem2020;
			attribute<string> 								WijkCode				:= src/wijk2020;
			attribute<string> 								BuurtCode				:= src/buurt2020;
			attribute<string> 								BuurtWoningTypeCode		:= BuurtCode + "_" + src/Woningtype;
			
			attribute<Geography/RegioIndelingen/gemeente>	Gemeente_rel 			:= rlookup(src/gem2020, Geography/RegioIndelingen/gemeente/GM_code);
			attribute<Geography/RegioIndelingen/wijk> 		Wijk_rel 				:= rlookup(src/wijk2020, Geography/RegioIndelingen/wijk/name);
			attribute<Geography/RegioIndelingen/buurt> 		Buurt_rel 				:= rlookup(src/buurt2020, Geography/RegioIndelingen/buurt/BU_code);
			attribute<Classifications/woningtypeBestaand> 	Woningtype_rel 			:= rlookup(src/Woningtype[uint8], Classifications/woningtypeBestaand/id);
			attribute<ratio>								LokalePraktijkFactor	:= src/verschilfactor[ratio], IntegrityCheck = "this > 0[ratio]";
			
			attribute<string>								GW_rel_str 				:= string(Gemeente_rel) + "_" + string(Wijk_rel);
			attribute<class/BW> 							BW_rel					:= combine_data(class/BW, 	Buurt_rel, 	Woningtype_rel);

			
			//attribute<class/GW> 							GW_rel					:= combine_data(class/GW, 	Gemeente_rel, 	Wijk_rel);
			//attribute<class/GWB> 							GWB_rel					:= combine_data(class/GWB, 	GW_rel, 		Buurt_rel);
			//attribute<class/GWBW> 							GWBW_rel				:= combine_data(class/GWBW, GWB_rel, 		Woningtype_rel);
		
		}
		
	}
	
	container EnergieLabel
	{
		parameter<bool> HerberekenStartsituatie := False;
		// Voor koppeling aan de BAG
		unit<uint32> nummeraanduiding := RuimtelijkeData/BAG/import/nummeraanduiding;
		unit<uint32> vbo              := RuimtelijkeData/BAG/import/vbo;

		unit<uint32> src
		:	StorageName     = "%SourceDataProjDir%/vraag/wonen/v20240201_v2_csv.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True"
		,	url             = "%SourceDataProjDir%/vraag/wonen20200106_Metadata_RVOlabels_FvdM.txt"
		{
			attribute<string> HLT_unquoted := trim(replace(Pand_huisletter,'''',''));
			attribute<string> adres_key    := 
				//lowercase(
					replace(Pand_postcode,'''','') + '_' + replace(Pand_huisnummer,'''','') + 
					(strlen(HLT_unquoted) > 0 
						? '_' + HLT_unquoted
						: ''
					)
				//)
				;
			unit<uint32> UniqueLabel := unique(Pand_energieklasse), DisableStorage="True" { attribute<uint32> Count := pcount(rlookup(Pand_energieklasse, values)); }
			
			//order of replace matters!
			attribute<Classifications/SchilLabel>   schillabel_rel := rlookup(replace(Pand_energieklasse, '+',''), Classifications/SchilLabel/name);
			attribute<uint32>                       datum_numeriek := uint32(replace(Pand_opnamedatum,'''',''));
			
		}
		unit<uint32> data_onlyDefined := select_with_attr_by_cond(src, IsDefined(src/schillabel_rel));

		unit<uint32> data_impl := unique(data_onlyDefined/adres_key)
		{
			attribute<string>                     nummeraanduiding_id := nummeraanduiding/identificatie[rlookup(values, nummeraanduiding/adres_key)];
			attribute<string>                     vbo_id              := rjoin(nummeraanduiding_id, vbo/nummeraanduiding_id, vbo/identificatie), FreeData = "False";
			attribute<uint32>                     datum_laatste       := max(data_onlyDefined/datum_numeriek, rlookup(data_onlyDefined/adres_key, values));
			attribute<Classifications/SchilLabel> SchilLabel_rel      := rjoin(values + '_' + string(datum_laatste), data_onlyDefined/adres_key + '_' + string(data_onlyDefined/datum_numeriek), data_onlyDefined/schillabel_rel), FreeData = "False", IntegrityCheck = "IsDefined(this)";
		}
		
		container LabelToFSS : IsHidden = "True"
		{
			unit<uint32> vbo := BAG/import/VBO
			{
				unit<uint32> Label_writer := vbo, Storagename = "%SourceDataProjDir%/vraag/Wonen/Schillabels.fss"
				{
					attribute<string>						vbo_id			    := data_impl/vbo_id[rlookup(identificatie, data_impl/vbo_id)];
					attribute<Classifications/SchilLabel>   SchilLabel_rel      := data_impl/SchilLabel_rel[rlookup(identificatie, data_impl/vbo_id)];
					attribute<uint32>  					    datum_laatste       := data_impl/datum_laatste[rlookup(identificatie, data_impl/vbo_id)];
					attribute<string>                       nummeraanduiding_id := data_impl/nummeraanduiding_id[rlookup(identificatie, data_impl/vbo_id)];

				}
				unit<uint32> Label_reader := vbo, StorageName = "%SourceDataProjDir%/vraag/Wonen/Schillabels.fss", StorageReadOnly = "true"
				{
					attribute<string>						vbo_id;
					attribute<Classifications/SchilLabel>   SchilLabel_rel;
					attribute<uint32>  					    datum_laatste;
					attribute<string>                       nummeraanduiding_id;
				}
			}
		}
		
		
		unit<uint32> data := = HerberekenStartsituatie ?  'LabelToFSS/vbo/Label_writer' : 'LabelToFSS/vbo/Label_reader';

