	#include <WarmteBronnen.dms>
	
	container buislengte 
	{
		unit<uint32> vesta_pand := select( BAG/import/pand/HeeftVestaAansluiting )
		{
			attribute<rdc_meter> geometry(poly) := collect_by_cond(vesta_pand, BAG/import/pand/geometry);
			attribute<rdc_meter> point := centroid_or_mid(geometry);
			attribute<Studiegebied/buurt> buurt_rel := point_in_polygon(point, Studiegebied/buurt/geometry);
		}
		unit<uint32> OSM_wegen := select(OSM/Road_Network/rank > 0b)
		{
			
			attribute<rdc_meter> geometry(arc) := collect_by_cond(OSM_wegen, OSM/Road_Network/geometry);
			container connect_info := connect_info(geometry, vesta_pand/point, sqr(80.0));
			attribute<nrAsl> nrAansluitingen := pcount(connect_info/Arc_rel)[nrAsl];
			attribute<m> length := mls_length(geometry, m);
			attribute<m_asl> distributie_per_aansl :=  length / nrAansluitingen;
			attribute<bool> tel_deze_mee := distributie_per_aansl  <= 100[m_asl]; // ten minste 1 aansluiting per 100m.
		}
		unit<uint32> OSM_wegen_met_aansluitingen := select(OSM_wegen/tel_deze_mee)
		{
			attribute<rdc_meter> geometry(arc) := collect_by_cond(OSM_wegen_met_aansluitingen, OSM_wegen/geometry);
			attribute<m>         arc_length := mls_length(geometry, m);
			attribute<rdc_meter> road_polygons(poly) := bg_buffer_linestring(geometry, 1[m], 0b);
			attribute<m2>        area := area(road_polygons, m2);
		}
		
		unit<uint32> wegen_per_buurt := bg_overlay_polygon(OSM_wegen_met_aansluitingen/road_polygons, Studiegebied/buurt/geometry)
		{
			attribute<m2> area := area(geometry, m2);
		}
		
		unit<uint32> buislengte_calculate := Studiegebied/buurt
		,	StorageName     = "%SourceDataProjDir%/infra/20240101_buislengte.csv"
		,	StorageType     = "gdalwrite.vect"
		,	StorageReadOnly = "False"
		,	url             = "%SourceDataProjDir%/infra/20240101_metadata_buislengte.pdf"
		{
			
			attribute<Geography/rdc_meter> geometry (poly) := Studiegebied/buurt/Geometry;
			attribute<string>              BU_CODE         := Studiegebied/buurt/BU_CODE;
			
			attribute<m2>      weg_area        := sum(wegen_per_buurt/area, wegen_per_buurt/second_rel);

			attribute<m>       L_dist_input    := weg_area / 2[m];
			attribute<nrPand>  nrPanden        := pcount(vesta_pand/buurt_rel)[nrPand]; // TODO: tel niet mee als er geen aansluiting is
			attribute<nrAsl >  nrAansluitingen := sum_uint32(IsDefined(OSM_wegen/connect_info/Arc_rel), vesta_pand/buurt_rel)[nrAsl]; // TODO: tel niet mee als er geen aansluiting is
			attribute<m>       L_aansl         := sum(OSM_wegen/connect_info/dist[m]*Float64(IsDefined(OSM_wegen/connect_info/Arc_rel)), vesta_pand/buurt_rel);
//			attribute<m>       L_dist          ; 
			attribute<m_asl>   L_aansl_pand     := max_elem(L_aansl / nrAansluitingen, 1[m_asl]);
			attribute<nrPand>  Niet_aangesloten := nrPanden - nrAansluitingen * 1[nrPand / nrAsl];
		}
		
		unit<uint32> buislengte_input_2020
		:	StorageName     = "%SourceDataProjDir%/infra/20200220_buislengte.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True"
		,	url             = "%SourceDataProjDir%/infra/20190618_metadata_buislengte.pdf"
		{
			attribute<m>       L_dist_input     := Hoofdleiding_lengte_m[m];
			attribute<m>       L_Aansl          := Aansluit_lengte_m[m];
			attribute<nrPand>  nrAansluitingen  := Aansluitingen[units/nrPand];
		}

		unit<uint32> buislengte_input
		:	StorageName     = "%SourceDataProjDir%/infra/20240101_buislengte.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True"
		,	url             = "%SourceDataProjDir%/infra/20240101_metadata_buislengte.pdf"
		{
			attribute<nrPand> nrAansluitingen;
			attribute<m>      L_dist_input;
			attribute<m>      L_Aansl;
		}
		

		unit<uint32> per_buurt_2020 := Studiegebied/buurt
		{
			attribute<Geography/rdc_meter> geometry (poly) := Studiegebied/buurt/Geometry;
			attribute<string>              BU_CODE         := Studiegebied/buurt/BU_CODE;

			attribute<m>       L_dist_input     := rjoin(BU_CODE, buislengte_input_2020/BU_CODE, buislengte_input_2020/Hoofdleiding_lengte_m[m]);
			attribute<nrPand>  nrAansluitingen  := rjoin(BU_CODE, buislengte_input_2020/BU_CODE, buislengte_input_2020/Aansluitingen[units/nrPand]);

			attribute<m>       L_aansl          := max_elem(rjoin(BU_CODE, buislengte_input_2020/BU_CODE, buislengte_input_2020/Aansluit_lengte_m[m]), nrPanden * 1[m_pand]), Descr = "invoer lengte aansluitleidingen per buurt";
			attribute<m>       L_dist           := max_elem(rjoin(BU_CODE, buislengte_input_2020/BU_CODE, buislengte_input_2020/Hoofdleiding_lengte_m[m]), 500[m])          , Descr = "invoer lengte distributieleidingen per buurt";
			attribute<nrPand>  nrPanden         := max_elem(rjoin(BU_CODE, buislengte_input_2020/BU_CODE, buislengte_input_2020/Panden[units/nrPand]), 1[units/nrPand])   , Descr = "invoer aantal panden per buurt";
			attribute<m_pand>  L_aansl_pand     := max_elem(L_aansl / nrPanden, 1[m_pand])                                                                        , Descr = "invoer gemiddelde lengte aansluitleidingen per pand";
			attribute<float64> Niet_aangesloten := float64(nrPanden) - float64(nrAansluitingen);
		}
		unit<uint32> per_buurt := Studiegebied/buurt
		{
			attribute<Geography/rdc_meter> geometry (poly) := Studiegebied/buurt/Geometry;
			attribute<string>              BU_CODE         := Studiegebied/buurt/BU_CODE;

			attribute<m>       L_dist_input     := rjoin(BU_CODE, buislengte_input/BU_CODE, buislengte_input/L_dist_input[m]);
			attribute<nrPand>  nrAansluitingen  := rjoin(BU_CODE, buislengte_input/BU_CODE, buislengte_input/nrAansluitingen[units/nrPand]);

			attribute<m>       L_aansl          := max_elem(rjoin(BU_CODE, buislengte_input/BU_CODE, buislengte_input/L_aansl[m]), nrAansluitingen * 1[m_pand]), Descr = "invoer lengte aansluitleidingen per buurt";
			attribute<m>       L_dist           := max_elem(L_dist_input, 500[m])          , Descr = "invoer lengte distributieleidingen per buurt";
			attribute<nrPand>  nrPanden         := max_elem(rjoin(BU_CODE, buislengte_input/BU_CODE, buislengte_input/nrPanden[units/nrPand]), 1[units/nrPand])   , Descr = "invoer aantal panden per buurt";
			attribute<m_pand>  L_aansl_pand     := max_elem(L_aansl / nrPanden, 1[m_pand])                                                                        , Descr = "invoer gemiddelde lengte aansluitleidingen per pand";
			attribute<float64> Niet_aangesloten := float64(nrPanden) - float64(nrAansluitingen);
		}
	}
	#include <OSM.dms>
	container infra
	{
		
		unit<uint32> netwerk_input_2022
		:	StorageName     = "%SourceDataProjDir%/infra/20241010_E_G_infra_CBS2022.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True"
		,	url             = "%SourceDataProjDir%/infra/20241010_E_G_infra_metadata.txt"
		,  DialogType = "Map", DialogData = "buurt_rel"
		{
			attribute<Geography/RegioIndelingen/Buurt2022> buurt_rel := rlookup(BU_CODE_2022, Geography/RegioIndelingen/Buurt2022/BU_CODE);
			
			attribute<nrAsl> input_aant_g_aansl         := replace(aant_g_aansl,',','.')[nrAsl];
			attribute<m>     input_lengte_grogel_LD_net := replace(lengte_grogel_LD_net_meters_,',','.')[m];
			attribute<m>     input_lengte_LD_net        := replace(lengte_LD_net_meters_,',','.')[m];
			attribute<ratio> fractie_3x25_aansl_ratio   := replace(fractie_3x25_aansl,',','.')[ratio];
			attribute<kw>    input_capaciteit_buurt     := replace(capaciteit_buurt_kva_,',','.')[kw];
			attribute<m>     input_lengte_LS_buurt      := replace(lengte_LS_buurt_meters_,',','.')[m];
		}
		
		unit<uint32> netwerk_input_2023
		:	StorageName     = "%SourceDataProjDir%/infra/20241010_E_G_infra_CBS2023.csv"
		,	StorageType     = "gdal.vect"
		,	StorageReadOnly = "True"
		,	url             = "%SourceDataProjDir%/infra/20241010_E_G_infra_metadata.txt"
		,  DialogType = "Map", DialogData = "buurt_rel"
		{
			attribute<Buurt2023> buurt_rel := rlookup(BU_CODE_2023, Geography/RegioIndelingen/Buurt2023/BU_CODE);
			
			attribute<nrAsl> input_aant_g_aansl         := replace(aant_g_aansl,',','.')[nrAsl];
			attribute<m>     input_lengte_grogel_LD_net := replace(lengte_grogel_LD_net_meters_,',','.')[m];
			attribute<m>     input_lengte_LD_net        := replace(lengte_LD_net_meters_,',','.')[m];
			attribute<ratio> fractie_3x25_aansl_ratio   := replace(fractie_3x25_aansl,',','.')[ratio];
			attribute<kw>    input_capaciteit_buurt     := replace(capaciteit_buurt_kva_,',','.')[kw];
			attribute<m>     input_lengte_LS_buurt      := replace(lengte_LS_buurt_meters_,',','.')[m];

		}
		
		unit<uint32> buurt2022 := Geography/RegioIndelingen/Buurt2022
		{
			attribute<netwerk_input_2022> input_rel := rlookup(BU_CODE, netwerk_input_2022/BU_CODE_2022);
			
			attribute<nrAsl> input_aant_g_aansl         := input_rel->input_aant_g_aansl;
			attribute<m>     input_lengte_grogel_LD_net := input_rel->input_lengte_grogel_LD_net;
			attribute<m>     input_lengte_LD_net        := input_rel->input_lengte_LD_net;
			attribute<ratio> fractie_3x25_aansl_ratio   := input_rel->fractie_3x25_aansl_ratio;
			attribute<kw>    input_capaciteit_buurt     := input_rel->input_capaciteit_buurt;
			attribute<m>     input_lengte_LS_buurt      := input_rel->input_lengte_LS_buurt;
		}
		
		

		unit<uint32> buurt2023 := Geography/RegioIndelingen/Buurt2023
		{
			attribute<netwerk_input_2023> input_rel := rlookup(BU_CODE, netwerk_input_2023/BU_CODE_2023);
			
			unit<uint32> xtab := /Geography/RegioIndelingen/Buurt2022x2023;
			
			attribute<nrAsl> input_aant_g_aansl_2022         := sum(buurt2022/input_rel->input_aant_g_aansl        [xtab/first_rel] * xtab/OS_FractionOf_2022    , xtab/second_rel);
			attribute<m>     input_lengte_grogel_LD_net_2022 := sum(buurt2022/input_rel->input_lengte_grogel_LD_net[xtab/first_rel] * xtab/Length_FractionOf_2022, xtab/second_rel);
			attribute<m>     input_lengte_LD_net_2022        := sum(buurt2022/input_rel->input_lengte_LD_net       [xtab/first_rel] * xtab/Length_FractionOf_2022, xtab/second_rel);
			attribute<ratio> fractie_3x25_aansl_ratio_2022   := sum(buurt2022/input_rel->fractie_3x25_aansl_ratio  [xtab/first_rel] * xtab/OS_FractionOf_2023    , xtab/second_rel); // fracties wegen naar bijdrage in bestemming
			attribute<kw>    input_capaciteit_buurt_2022     := sum(buurt2022/input_rel->input_capaciteit_buurt    [xtab/first_rel] * xtab/OS_FractionOf_2022    , xtab/second_rel);
			attribute<m>     input_lengte_LS_buurt_2022      := sum(buurt2022/input_rel->input_lengte_LS_buurt     [xtab/first_rel] * xtab/Length_FractionOf_2022, xtab/second_rel);
			
			attribute<nrAsl> input_aant_g_aansl_2023         := input_rel->input_aant_g_aansl;
			attribute<m>     input_lengte_grogel_LD_net_2023 := input_rel->input_lengte_grogel_LD_net;
			attribute<m>     input_lengte_LD_net_2023        := input_rel->input_lengte_LD_net;
			attribute<ratio> fractie_3x25_aansl_ratio_2023   := input_rel->fractie_3x25_aansl_ratio;
			attribute<kw>    input_capaciteit_buurt_2023     := input_rel->input_capaciteit_buurt;
			attribute<m>     input_lengte_LS_buurt_2023      := input_rel->input_lengte_LS_buurt;

			
			// toedeling o.b.v. "Overzicht_buurtindelingen" in "202405080_E_G_infra_CBS2023_v2.csv"; bron Capaciteit n.n.t.b.
			attribute<nrAsl> input_aant_g_aansl         := max_elem(makedefined(input_aant_g_aansl_2023,0[nrAsl])   , input_aant_g_aansl_2022);
			attribute<m>     input_lengte_grogel_LD_net := max_elem(makedefined(input_lengte_grogel_LD_net_2023,0[m]) , input_lengte_grogel_LD_net_2022);
			attribute<m>     input_lengte_LD_net        := max_elem(makedefined(input_lengte_LD_net_2023,0[m])        , input_lengte_LD_net_2022);
			attribute<ratio> fractie_3x25_aansl_ratio   := max_elem(makedefined(fractie_3x25_aansl_ratio_2023,0d)   , fractie_3x25_aansl_ratio_2022);
			attribute<kw>    input_capaciteit_buurt     := max_elem(makedefined(input_capaciteit_buurt_2023,0[kw])     , input_capaciteit_buurt_2022);
			attribute<m>     input_lengte_LS_buurt      := max_elem(makedefined(input_lengte_LS_buurt_2023,0[m])      , input_lengte_LS_buurt_2022);
		}
		
		unit<uint32> per_buurt := Studiegebied/buurt
		{
			attribute<Geography/rdc_meter> geometry (poly) := Studiegebied/buurt/Geometry;
			attribute<string>              BU_CODE         := Studiegebied/buurt/BU_CODE;

			attribute<buurt2023> buurt_rel := rlookup(BU_CODE, buurt2023/BU_CODE);
			
			container BC := TussenResultaten/StartJaar/BebouwingsComponenten;

			attribute<nrAsl> input_aant_g_aansl            := buurt_rel->input_aant_g_aansl, Descr = "invoer aantal gasaansluitingen per buurt";

			attribute<nrAsl> default_aant_g_aansl          := 
				= AsList('sum(BC/Bestaande' + /Classifications/BebouwingsSector/name + '/BO/nrAansluitingen, BC/Bestaande' + /Classifications/BebouwingsSector/name + '/BO/PlanRegio_rel)', ' + ');

			attribute<nrAsl> aant_g_aansl                  := IsDefined(input_aant_g_aansl) ? input_aant_g_aansl : default_aant_g_aansl;
			attribute<nrAsl> verschil_g_aansl              := aant_g_aansl - default_aant_g_aansl;

			attribute<m>     input_lengte_grogel_LD_net    := buurt_rel->input_lengte_grogel_LD_net, Descr = "invoer lengte grondroeringsgevoelige LD-gasleidingen per buurt";
			attribute<m>     default_lengte_grogel_LD_net  := const(0[m],.), Descr = "default lengte grondroeringsgevoelige LD-gasleidingen per buurt";
			attribute<m>     lengte_grogel_LD_net          := IsDefined(input_lengte_grogel_LD_net) ? input_lengte_grogel_LD_net : default_lengte_grogel_LD_net;
			attribute<m>     verschil_lengte_grogel_LD_net := lengte_grogel_LD_net - default_lengte_grogel_LD_net;

			attribute<m>     input_lengte_LD_net           := buurt_rel->input_lengte_LD_net, Descr = "invoer lengte LD-gasnet per buurt";
			attribute<m>     default_lengte_LD_net         := TussenResultaten/StartJaar/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/L_distr_g 
				* float64(TussenResultaten/StartJaar/StateNaAllocatie/Infra_per_buurt/aant_g_aansl > 0[nrAsl]);
			attribute<m>     lengte_LD_net                 := IsDefined(input_lengte_LD_net) && IsDefined(input_aant_g_aansl) ? input_lengte_LD_net : default_lengte_LD_net;
			attribute<m>     verschil_g_lengte_LD_net      := lengte_LD_net - default_lengte_LD_net;

			attribute<ratio> input_fractie_3x25_aansl      := buurt_rel->fractie_3x25_aansl_ratio, Descr = "invoer aandeel van aansluitingen per buurt dat momenteel al 3x25 of groter is";
			attribute<ratio> default_fractie_3x25_aansl    := const(0[ratio],.), Descr = "default aandeel van aansluitingen per buurt dat momenteel al 3x25 of groter is";
			attribute<ratio> fractie_3x25_aansl            := IsDefined(input_fractie_3x25_aansl) ? input_fractie_3x25_aansl : default_fractie_3x25_aansl;
			attribute<ratio> verschil_fractie_3x25_aansl   := fractie_3x25_aansl - default_fractie_3x25_aansl;

			attribute<kw>    input_capaciteit_buurt        := buurt_rel->input_capaciteit_buurt, Descr = "invoer huidige opgestelde capaciteit aan MS-ruimtes ";

			attribute<kw>    default_capaciteit_buurt      :=
				= AsList('sum(BC/Bestaande' + /Classifications/BebouwingsSector/name + '/BO/AansluitCapaciteit/ASW_Eapp, BC/Bestaande' + /Classifications/BebouwingsSector/name + '/BO/PlanRegio_rel) 
						  * TussenResultaten/StartJaar/Vermogens/GTF_' + /Classifications/BebouwingsSector/NcwRefBase + '_Enet  ', ' + ')
			, Descr = "default huidige opgestelde capaciteit aan MS-ruimtes";
			
			attribute<kw>    capaciteit_buurt              := IsDefined(input_capaciteit_buurt) ? (input_capaciteit_buurt > default_capaciteit_buurt ? input_capaciteit_buurt : default_capaciteit_buurt) : default_capaciteit_buurt;
			attribute<kW>    verschil_capaciteit_buurt     := capaciteit_buurt - default_capaciteit_buurt;

			attribute<m>     input_lengte_LS_net           := buurt_rel->input_lengte_LS_buurt,     Descr = "invoer totale lengte van het LS-net per buurt";
			attribute<m>     default_lengte_LS_net         := TussenResultaten/StartJaar/GebiedsAllocatie/RestWarmte/KostenBaten/KostenD/L_infra_e, Descr = "default totale lengte van het LS-net per buurt";
			attribute<m>     lengte_LS_net                 := IsDefined(input_lengte_LS_net) ? input_lengte_LS_net : default_lengte_LS_net;
			attribute<m>     verschil_lengte_LS_net        := lengte_LS_net - default_lengte_LS_net;
		}
	}

	// KNMI klimaatscenario voor daling functionele warmtevraag in de toekomst door hogere gemiddelde buitentemperatuur
	// Beschikbare varianten:
	// "%sourceDataProjDir%/hulpbestanden/klimaat/GL/GL_' + Classifications/ZichtJaar/label + '.tif"     ( + 1,0 graden celcius wereldwijd in 2050 )
	// "%sourceDataProjDir%/hulpbestanden/klimaat/GH/GH_' + Classifications/ZichtJaar/label + '.tif"     ( + 1,4 graden celcius wereldwijd in 2050 )
	// "%sourceDataProjDir%/hulpbestanden/klimaat/WL/WL_' + Classifications/ZichtJaar/label + '.tif"     ( + 2,0 graden celcius wereldwijd in 2050 )
	// "%sourceDataProjDir%/hulpbestanden/klimaat/WH/WH_' + Classifications/ZichtJaar/label + '.tif"     ( + 2,3 graden celcius wereldwijd in 2050 )

	container Klimaat :=
			for_each_ndva(
				 Classifications/ZichtJaar/name
				,Geography/rdc_grids/m100
				,float32
				,'%sourceDataProjDir%/hulpbestanden/klimaat/GH/GH_' + Classifications/ZichtJaar/label + '.tif'
			)
			, descr = "Klimaat_scenario_knmi"
			, url   = "%sourceDataProjDir%/hulpbestanden/klimaat/Werkwijze_aanmaak_correctiekaarten_klimaat_obv_KNMI2014_scenarios_tbv_Vesta33.pdf";

	// inlezen kerncijfers wijken en buurten - meerdere jaren om missende waarden te ondervangen
	container CBSWijkEnBuurt: url="%sourceDataProjDir%/CBS/20200129_update_kerncijfers_wijken_buurten_BVB.txt"
	{
		unit<uint32> jaar := range(uint32,2013,2024)
		{
			attribute<string> filename:['20180802_kwb-2013','20180802_kwb-2014','20180802_kwb-2015','20190118_kwb-2016','20190118_kwb-2017','20190118_kwb-2018','20200129_kwb-2019', '20240516_kwb-2020', '20240516_kwb-2021', '20240516_kwb-2022', '20240516_kwb-2023'];
			attribute<string> reverse := string(max(id(.)) - (id(.) - min(id(.))));
		}
		container per_jaar: = for_each_ne('j' + string(id(jaar)), 'src_per_year(' + quote(string(id(jaar))) + ',' + quote(jaar/filename)+ ')');
		template src_per_year
		{
			// begin case parameters
			parameter<string> year;
			parameter<string> filename;
			// end case parameter

			unit<uint32> src:
				StorageName     = "='%SourceDataProjDir%/CBS/' + filename + '.csv'",
				StorageType     = "gdal.vect",
				StorageReadOnly = "True";
		}
	}

