		container VT //Ventilatie
		{
			unit<uint32> Installatie_imp: StorageName = "%projDir%/installaties/Ventilatie.csv", StorageType = "gdal2.vect", StorageReadOnly = "True"
			{
				attribute<string> name; // PrimaryKey
			}
			unit<uint32> Installatietypes := Installatie_imp
			{
				attribute<string>    name       := Installatie_imp/name;
				attribute<string>    label      := Installatie_imp/label;
				attribute<yr_uint16> Startjaar  := Installatie_imp/startjaar[yr_uint16];
				attribute<yr_uint16> Eindjaar   := Installatie_imp/eindjaar[yr_uint16];
				attribute<GJ_yr_asl> V_RV_asl   := Installatie_imp/Vrv_asl[GJ_yr_asl];
				attribute<GJ_yr_asl> V_E_asl    := Installatie_imp/Ve_asl[GJ_yr_asl];
				attribute<yr_uint16> Levensduur := Installatie_imp/AT[yr_uint16];
				attribute<float64>   P50P       := Installatie_imp/P50P[float64];
				attribute<float64>   BETA       := Installatie_imp/BETA[float64];

				unit<uint32> InstallatieMaatregel : StorageName = "%projDir%/installaties/VentilatieMaatregel.csv", StorageType = "gdal2.vect", StorageReadOnly = "True"
				{
					attribute<string> name := Inst_voor + '_x_' + Inst_na;
					attribute<Installatietypes> Installatie_voor := rlookup(Inst_voor, Installatietypes/name);
					attribute<Installatietypes> Installatie_na   := rlookup(Inst_na  , Installatietypes/name);

					container ki_asl := for_each_nedv(Classifications/Uitvoering/Name, 'InstallatieMaatregel/'+Classifications/Uitvoering/Name+'[Eur_asl]', InstallatieMaatregel, EUR_asl);
				}
				
				attribute<.>    GasVervanger     := ID(.);
				attribute<bool> GeenGasvervanger := Gasvervanger == ID(.);
				
				container V := for_each_nedv(name, 'value('+string(id(.))+', ..)', void, .);
			}
		}
