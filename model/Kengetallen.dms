				attribute<Classifications/BemeterdeGebouwInput> BemeterdeGebouwInput_rel : [1,0];
				
				attribute<.>     GasVervanger     := union_data(., V/Elek, V/Elek);
				attribute<bool>  GeenGasvervanger := Gasvervanger == ID(.);	

				parameter<GJ_yr>     Vkk_prs := 0.40[ratio] * 0.296[GJ_yr]; //0.4 = SPF gas
				parameter<GJ_yr_asl> Vkk_asl := 0.40[ratio] * 0.737[GJ_yr_asl]; //0.4 = SPF gas
